      "parameters": {
        "jsCode": "// 1. OBTENER LOS LEADS VIEJOS\n\n// para acceder solamente a los datos provenientes de ese nodo.\nconst existingLeads = $items(\"Get row(s) in sheet\");\n\nconst existingDomains = existingLeads.map(item => {\n\n    return normalizeDomain(item.json.Dominio); \n});\n\nconst existingSet = new Set(existingDomains);\n\n\nconst newLeads = $items(\"Run an Actor and get dataset\");\n\n// Función para limpiar URLS (quita http, www, etc para comparar bien)\nfunction normalizeDomain(url) {\n    if (!url) return \"\";\n    try {\n        // Si no tiene protocolo, le agregamos https para que URL() no falle\n        if (!url.startsWith('http')) url = 'https://' + url;\n        let domain = new URL(url).hostname;\n        return domain.replace('www.', '').toLowerCase();\n    } catch (e) {\n        // En caso de error (URL inválida), devuelve la URL original en minúsculas\n        return String(url).toLowerCase();\n    }\n}\n\n// 3. FILTRAR\nconst filteredLeads = newLeads.filter(leadItem => {\n    // Intenta usar 'website' o 'url' del Apify lead\n    const apifyUrl = leadItem.json.website || leadItem.json.url; \n    const normalizedApify = normalizeDomain(apifyUrl);\n\n    // Si el dominio ya existe en el Set de Google Sheets, lo descartamos (retorna false)\n    return !existingSet.has(normalizedApify);\n});\n\nreturn filteredLeads;"
      },
